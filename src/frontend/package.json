{
  "$schema": "./node_modules/@types/package.json",
  "name": "expo-icp",
  "description": "This is a template project that enables the use of Internet Identity in Expo Web and Native apps.",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm install && npx expo install --fix",
    "start": "npx expo install --fix && expo start --go --clear",
    "start:dev-client": " npx expo-doctor && EXPO_PUBLIC_EAS_DEEP_LINK_TYPE=legacy expo start --dev-client --clear",
    "build": "npx expo export -p web",
    "eas:init": "eas init",
    "eas:device:create": "eas device:create",
    "eas:credentials": "eas credentials",
    "setup-env": "DFX_NETWORK=ic node ../../scripts/setup-env.js",
    "eas:build:ios:preview": "npm run setup-env && tsc && eas build --profile preview --platform ios",
    "eas:build:android:preview": "npm run setup-env && tsc && eas build --profile preview --platform android",
    "eas:build:ios:production": "npm run setup-env && tsc && eas build --profile production --platform ios",
    "eas:build:android:production": "npm run setup-env && tsc && eas build --profile production --platform android",
    "test": "node --experimental-vm-modules node_modules/vitest/vitest.mjs",
    "test:watch": "node --experimental-vm-modules node_modules/vitest/vitest.mjs watch",
    "test:coverage": "node --experimental-vm-modules node_modules/vitest/vitest.mjs run --coverage"
  },
  "dependencies": {
    "@dfinity/agent": "file:dfinity-agent-0.20.2.tgz",
    "@dfinity/auth-client": "^0.20.2",
    "@dfinity/bls-verify": "file:dfinity-bls-verify-0.20.2.tgz",
    "@dfinity/candid": "^0.20.2",
    "@dfinity/identity": "^0.20.2",
    "@dfinity/principal": "^0.20.2",
    "@expo/vector-icons": "^14.1.0",
    "@noble/curves": "file:noble-curves-1.2.0.tgz",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "canister-manager": "^0.1.7",
    "expo": "^53.0.9",
    "expo-blur": "~14.1.4",
    "expo-clipboard": "~7.1.4",
    "expo-constants": "~17.1.5",
    "expo-crypto-universal": "^0.2.7",
    "expo-crypto-universal-native": "^0.2.7",
    "expo-crypto-universal-web": "^0.2.7",
    "expo-dev-client": "~5.1.8",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-icp-app-connect": "^0.1.0",
    "expo-icp-app-connect-helpers": "^0.1.2",
    "expo-icp-frontend-helpers": "^0.1.12",
    "expo-ii-integration": "^0.1.21",
    "expo-image": "~2.1.7",
    "expo-linking": "~7.1.4",
    "expo-router": "~5.0.6",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-storage-universal": "^0.3.5",
    "expo-storage-universal-native": "^0.3.5",
    "expo-storage-universal-web": "^0.3.5",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3",
    "vitest": "^2.1.8"
  },
  "private": true,
  "resolutions": {
    "@dfinity/agent": "0.20.2",
    "@dfinity/candid": "0.20.2",
    "@dfinity/principal": "0.20.2",
    "@noble/hashes": "1.8.0"
  },
  "overrides": {
    "@noble/hashes": "1.8.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false,
        "exclude": []
      }
    }
  }
}
